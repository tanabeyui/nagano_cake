


<%= form_with model: @genre, url: admin_genre_path, method: :patch  do |f| %>
       <div class="form-group">
        <label_for="genre_name">Title</label>
        <%= f.text_field :name,  id: "genre_name" %>
      </div>
        <div class="form-group">
        <%= f.submit "Update Book", class: "btn btn-success" %>
        </div>

      <% end %>